<script>

function fn_fechar_modal()
{
      document.getElementById("modalCookie").className = "modal-dialog p-3 d-none";
      var dataAtual = new Date().toLocaleDateString();
      localStorage.setItem('ultimaDataModal', dataAtual);
}
</script>

